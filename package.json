{
  "name": "census-ro-backend",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn migrate-up && backpack",
    "dev-down": "yarn migrate-down && yarn migrate-up && backpack",
    "build": "rimraf build/ && backpack build",
    "migrate-down": "knex-migrate down --to 0",
    "migrate-up": "knex-migrate up",
    "test": "NODE_ENV=test yarn migrate-down && NODE_ENV=test yarn migrate-up --env test && jest --detectOpenHandles --runInBand --forceExit",
    "refresh": "bash cmds/refresh.sh",
    "grab": "bash cmds/grab.sh",
    "branch": "bash cmds/branch.sh",
    "start": "yarn migrate-up && yarn serve",
    "serve": "node build/main.js"
  },
  "dependencies": {
    "@jakelowen/sqorn-graphql-filters": "0.4.4",
    "@sendgrid/mail": "^6.3.1",
    "@sqorn/pg": "^0.0.45",
    "connect-redis": "^3.4.0",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^6.2.0",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "graphql": "^14.1.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-redis-subscriptions": "^2.0.0",
    "graphql-shield": "^5.1.3",
    "graphql-yoga": "^1.17.4",
    "ioredis": "^4.6.2",
    "knex": "^0.16.3",
    "knex-migrate": "^1.7.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "nodemailer": "^5.1.1",
    "obj-flatten": "^2.0.4",
    "pg": "^7.8.0",
    "pg-range": "^1.0.1",
    "slugify": "^1.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "backpack-core": "^0.8.3",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "faker": "^4.1.0",
    "graphql-middleware": "^3.0.2",
    "graphql-tools": "^4.0.4",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3"
  },
  "engines": {
    "node": "11.10.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "trashbin"
    ]
  },
  "heroku-run-build-script": true
}
